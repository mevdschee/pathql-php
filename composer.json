      "swagger",
      "automatic-api",
      "database",
      "multi-database",
      "sql-database",
      "ubuntu-linux"
    ],
    "homepage": "https://github.com/mevdschee/pathql-php",
    "license": "MIT",
    "authors": [
      {
        "name": "Maurits van der Schee",
        "email": "maurits@vdschee.nl",
        "homepage": "https://github.com/mevdschee"
      }
    ],
    "require": {
      "php": ">=7.0.0",
      "psr/http-message": "*",
      "psr/http-factory": "*",
